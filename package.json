{
    "name": "brainrot-judge",
    "version": "2.2.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "deploy": "npm run build && netlify deploy --prod",
        "deploy:all": "npm run deploy:worker && npm run deploy:frontend",
        "deploy:worker": "npx wrangler deploy",
        "deploy:frontend": "npm run build && netlify deploy --prod",
        "worker:dev": "wrangler dev",
        "db:migrate": "wrangler d1 execute brainrot-judge-db --remote --file=migrations/ai-enhancement.sql",
        "test:api": "node scripts/test-api.js",
        "populate:slang": "node scripts/populate-slang.js",
        "health": "node scripts/health-check.js",
        "check": "npm run health && npm run test:api"
    },
    "dependencies": {
        "lucide-react": "^0.263.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@vitejs/plugin-react": "^4.0.3",
        "autoprefixer": "^10.4.14",
        "postcss": "^8.4.27",
        "tailwindcss": "^3.3.3",
        "vite": "^7.1.4",
        "wrangler": "^4.33.2"
    }
}